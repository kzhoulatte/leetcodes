# Leet127: Word ladder
"""
Example:
Input:
beginWord = "hit",
endWord = "cog",
wordList = ["hot","dot","dog","lot","log","cog"]

Output: 5

"""

class Solution:
    def ladderLength(self, beginWord, endWord, wordList):

        if endWord not in wordList:
            return 0
        if len(beginWord) != len(endWord):
            return 0

        length = 20
        return length

solver = Solution()
print(solver.ladderLength('hit','cog',["hot","dot","dog","lot","log","cog"]))
